{
  "name": "js-kt",
  "description": "Kata for building JavaScript/TypeScript web servers with ease",
  "version": "1.6.15",
  "keywords": [
    "typescript",
    "nodejs",
    "web framework",
    "file-based routing",
    "websocket",
    "socket.io",
    "bun",
    "express",
    "type-safe",
    "api documentation",
    "cli",
    "web server"
  ],
  "type": "module",
  "license": "MIT",
  "bin": {
    "kt-cli": "dist/utils/cli/index.js"
  },
  "scripts": {
    "build": "rm -rf ./dist && tsc && chmod -R 755 ./dist && chmod +x ./dist/utils/cli/index.js",
    "prepare": "npm run build && echo 'Built successfully!'"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "server"
  ],
  "author": "Salem Elmotamed s.elmotamed96@gmail.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/almoatamed/js-katana.git"
  },
